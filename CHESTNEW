function thisCode()
    repeat task.wait() until game:IsLoaded()
    
    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer
    local HttpService = game:GetService("HttpService")
    local TeleportService = game:GetService("TeleportService")
    local PlaceID = game.PlaceId
    local AllIDs = {}
    local foundAnything = ""
    local actualHour = os.date("!*t").hour
    local Deleted = false

    local File = pcall(function()
        AllIDs = HttpService:JSONDecode(readfile("NotSameServers.json"))
    end)
    if not File then
        table.insert(AllIDs, actualHour)
        writefile("NotSameServers.json", HttpService:JSONEncode(AllIDs))
    end

    -- Fungsi untuk mencari dan menyentuh chest
    task.spawn(function()
        while true do
            -- Pastikan karakter ada
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                -- Cek keberadaan chest dalam urutan prioritas
                local Chest = game.Workspace:FindFirstChild("Chest4")
                    or game.Workspace:FindFirstChild("Chest3")
                    or game.Workspace:FindFirstChild("Chest2")
                    or game.Workspace:FindFirstChild("Chest")
                
                -- Jika chest ditemukan
                if Chest then
                    -- Pastikan karakter mendekatkan diri ke chest
                    LocalPlayer.Character:PivotTo(Chest:GetPivot())
                    task.wait(0.5) -- Tunggu sebentar untuk stabilitas
                    pcall(function()
                        -- Gunakan firetouchinterest untuk mensimulasikan interaksi
                        firetouchinterest(LocalPlayer.Character.HumanoidRootPart, Chest, 0)  -- Menyentuh
                        task.wait(0.1)
                        firetouchinterest(LocalPlayer.Character.HumanoidRootPart, Chest, 1)  -- Lepas sentuhan
                    end)
                else
                    print("Chest tidak ditemukan di server ini.")
                end
            end
            task.wait(1)  -- Tunggu sejenak sebelum mencoba lagi
        end
    end)

    -- Fitur notifikasi untuk enable/disable script
    local AllowRunService = true
    local AllowRunServiceBind = Instance.new("BindableFunction")
    function AllowRunServiceBind.OnInvoke(args)
        if args == "Enable" then
            AllowRunService = true
        elseif args == "Disable" then
            AllowRunService = false
        end
    end

    local CoreGui = game:GetService("StarterGui")
    CoreGui:SetCore("SendNotification", {
        Title = "Farm Chest Blox Fruits",
        Text = "This Script By SirenStore",
        Duration = math.huge,
        Callback = AllowRunServiceBind,
        Button1 = "Enable",
        Button2 = "Disable",
    })

    -- Loop utama untuk mengaktifkan/mematikan skrip
    task.spawn(function()
        while true do
            if AllowRunService then
                -- Pastikan karakter ada dan memulai interaksi dengan chest
                if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                    -- Periksa apakah ada tag CrewBBG yang perlu dihapus
                    local hasCrewTag = LocalPlayer.Character:FindFirstChild("CrewBBG", true)
                    if hasCrewTag then
                        hasCrewTag:Destroy()
                    end
                end
            end
            task.wait(1) -- Tunggu sejenak sebelum melanjutkan
        end
    end)
end

thisCode()
